<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="de.badresden.zasa">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".FinishedQuestionaires"></activity>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".QuestionnaireAllgemein"
            android:label="QuestionnaireAllgemein"
            android:parentActivityName=".MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MainActivity" />
        </activity>
        <activity
            android:name=".QuestionnaireKlassifizierung"
            android:label="QuestionnaireKlassifizierung"
            android:parentActivityName=".QuestionnaireAllgemein">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".QuestionnaireAllgemein" />
        </activity>
        <activity
            android:name=".QuestionnaireTragfaehigkeit"
            android:label="QuestionnaireKlassifizierung"
            android:parentActivityName=".QuestionnaireKlassifizierung">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".QuestionnaireKlassifizierung" />
        </activity>
        <activity
            android:name=".QuestionnaireGebrauchstauglichkeit"
            android:label="QuestionnaireKlassifizierung"
            android:parentActivityName=".QuestionnaireTragfaehigkeit">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".QuestionnaireTragfaehigkeit" />
        </activity>
        <activity
            android:name=".QuestionnaireDauerhaftigkeit"
            android:label="QuestionnaireKlassifizierung"
            android:parentActivityName=".QuestionnaireGebrauchstauglichkeit">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".QuestionnaireGebrauchstauglichkeit" />
        </activity>
    </application>
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />

</manifest>