<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="de.badresden.zasa">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".FinishedQuestionaires"></activity>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".FragebogenAllgemein"
            android:label="FragebogenAllgemein"
            android:parentActivityName=".MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MainActivity" />
        </activity>
        <activity
            android:name=".FragebogenKlassifizierung"
            android:label="FragebogenKlassifizierung"
            android:parentActivityName=".FragebogenAllgemein">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".FragebogenAllgemein" />
        </activity>
        <activity
            android:name=".FragebogenTragfaehigkeit"
            android:label="FragebogenTragfaehigkeit"
            android:parentActivityName=".FragebogenKlassifizierung">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".FragebogenKlassifizierung" />
        </activity>
        <activity
            android:name=".FragebogenGebrauchstauglichkeit"
            android:label="FragebogenGebrauchstauglichkeit"
            android:parentActivityName=".FragebogenTragfaehigkeit">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".FragebogenTragfaehigkeit" />
        </activity>
        <activity
            android:name=".FragebogenDauerhaftigkeit"
            android:label="FragebogenDauerhaftigkeit"
            android:parentActivityName=".FragebogenGebrauchstauglichkeit">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".FragebogenGebrauchstauglichkeit" />
        </activity>





    </application>

</manifest>